#!/bin/bash
# PATHS
export EUDAQ_SERVER=192.168.1.102
export RCPORT=44000
EUDAQDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
SOFTDIR="$(dirname "$EUDAQDIR")"
export EUDAQDIR=$EUDAQDIR
export PXARPATH="$SOFTDIR/pxar6"
export DRS_PATH="$SOFTDIR/DRS4-v5-shared"

# PROCESS ALIASES
cmspix="$EUDAQDIR/bin/CMSPixelProducer.exe -r $EUDAQ_SERVER:$RCPORT"
alias DataCollector='$EUDAQDIR/bin/TestDataCollector.exe -r $EUDAQ_SERVER:$RCPORT'
alias PixelProducer='$cmspix'
alias CMSPixel='$cmspix -n CMSPixel &'
alias CMSPixelDig='$cmspix -n CMSPixelDig'
alias DRS4Producer='$EUDAQDIR/bin/DRSProducer.exe -r $EUDAQ_SERVER:$RCPORT -n DRS4Producer &'

# ALIASES
alias OnlineMonitor='$EUDAQDIR/scripts/OnlineMon.py'
alias Convert='$EUDAQDIR/scripts/Converter.py'
alias DoTracking='$EUDAQDIR/scripts/DoTracking.py'
alias logger='~/scripts/logger.sh'
alias mountRapidshare='sudo mount 192.168.1.100:/mnt/raid /data'
alias restartDRS4='ykushcmd ykushxs -d; sleep .5; ykushcmd ykushxs -u'

