# PATHS
export EUDAQ_SERVER=192.168.1.102
export RCPORT=44000
export EUDAQDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
SOFTDIR="$(dirname $EUDAQDIR)"
export PXARPATH="$SOFTDIR/pxar_root6"
export DRS_PATH="$SOFTDIR/DRS4-v5-shared"

# PROCESS ALIASES
alias Collector='$EUDAQDIR/bin/TestDataCollector.exe -r $EUDAQ_SERVER:$RCPORT'
alias CMSPixel='$EUDAQDIR/bin/CMSPixelProducer.exe -r $EUDAQ_SERVER:$RCPORT -n CMSPixel &'
alias CMSPixelDig='$EUDAQDIR/bin/CMSPixelProducer.exe -r $EUDAQ_SERVER:$RCPORT -n CMSPixelDig'

# ALIASES
alias OnlineMonitor='$EUDAQDIR/scripts/OnlineMon.py'
alias Convert='$EUDAQDIR/scripts/Converter.py'
alias DoTracking='$EUDAQDIR/scripts/DoTracking.py'
alias logger='~/scripts/logger.sh'
alias mountRapidshare='sudo mount 192.168.1.100:/mnt/raid /data'


